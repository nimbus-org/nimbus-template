<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE server PUBLIC
  "-//Nimbus//DTD Nimbus 1.0//JA"
  "http://nimbus.sourceforge.jp/dtd/nimbus-service_1_0.dtd">

<server>
    
    <default-log>
        <debug output="false"/>
        <information output="false"/>
        <warning output="true"/>
        <error output="true"/>
        <fatal output="true"/>
    </default-log>
    
    <manager name="Sample.Service.KeepAlive.KeepAliveChecker">
        
        <manager-property name="Template.Service.KeepAlive.KeepAliveChecker.CONNECTION_FACTORY_SERVICE_NAME">Sample.Service.Connection#ConnectionFactory</manager-property>
        
        <service template="Template.Service.KeepAlive.KeepAliveChecker.JDBC#KeepAliveChecker"/>
        
    </manager>
</server>