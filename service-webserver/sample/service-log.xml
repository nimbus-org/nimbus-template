<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE server PUBLIC
  "-//Nimbus//DTD Nimbus 1.0//JA"
  "http://nimbus.sourceforge.jp/dtd/nimbus-service_1_0.dtd">

<server>
    
    <log manager-name="WebServer.Log">Logger</log>
    
    <manager name="WebServer.Log" shutdown-hook="true">
        
        <manager-property name="Template.Service.Log.LOG_OUTPUT_PATH">./target/</manager-property>
        <manager-property name="Template.Service.Log.CONTEXT_SERVICE_NAME">#Context</manager-property>
        
        <service template="Template.WebServer.Log#Context"/>
        
        <service template="Template.WebServer.Log#ThreadContext"/>
        
        <service template="Template.WebServer.Log#ServerInfo"/>
        
        <service template="Template.WebServer.Log#Logger"/>
        
        <service template="Template.WebServer.Log#LogMessageRecordFactory"/>
        <service template="Template.WebServer.Log#MessageRecordFactory"/>
        
        <service template="Template.WebServer.Log#LogFileWritableRecordFactory"/>
        <service template="Template.WebServer.Log#LogConsoleWritableRecordFactory"/>
        
        <service template="Template.WebServer.Log#FrameLogWriter"/>
        <service template="Template.WebServer.Log#ApplLogWriter"/>
        <service template="Template.WebServer.Log#ConsoleWriter"/>
        
        <service template="Template.WebServer.Log#FrameGroupCategory"/>
        <service template="Template.WebServer.Log#FrameFileCategory"/>
        <service template="Template.WebServer.Log#FrameConsoleCategory"/>
        
        <service template="Template.WebServer.Log#SysDebugGroupCategory"/>
        <service template="Template.WebServer.Log#SysDebugFileCategory"/>
        <service template="Template.WebServer.Log#SysDebugConsoleCategory"/>
        
        <service template="Template.WebServer.Log#SysInfoGroupCategory"/>
        <service template="Template.WebServer.Log#SysInfoFileCategory"/>
        <service template="Template.WebServer.Log#SysInfoConsoleCategory"/>
        
        <service template="Template.WebServer.Log#SysWarnGroupCategory"/>
        <service template="Template.WebServer.Log#SysWarnFileCategory"/>
        <service template="Template.WebServer.Log#SysWarnConsoleCategory"/>
        
        <service template="Template.WebServer.Log#SysErrorGroupCategory"/>
        <service template="Template.WebServer.Log#SysErrorFileCategory"/>
        <service template="Template.WebServer.Log#SysErrorConsoleCategory"/>
        
        <service template="Template.WebServer.Log#SysFatalGroupCategory"/>
        <service template="Template.WebServer.Log#SysFatalFileCategory"/>
        <service template="Template.WebServer.Log#SysFatalConsoleCategory"/>
        
        <service template="Template.WebServer.Log#ComTraceGroupCategory"/>
        <service template="Template.WebServer.Log#ComTraceFileCategory"/>
        <service template="Template.WebServer.Log#ComTraceConsoleCategory"/>
        
        <service template="Template.WebServer.Log#ComDebugGroupCategory"/>
        <service template="Template.WebServer.Log#ComDebugFileCategory"/>
        <service template="Template.WebServer.Log#ComDebugConsoleCategory"/>
        
        <service template="Template.WebServer.Log#ComInfoGroupCategory"/>
        <service template="Template.WebServer.Log#ComInfoFileCategory"/>
        <service template="Template.WebServer.Log#ComInfoConsoleCategory"/>
        
        <service template="Template.WebServer.Log#ComWarnGroupCategory"/>
        <service template="Template.WebServer.Log#ComWarnFileCategory"/>
        <service template="Template.WebServer.Log#ComWarnConsoleCategory"/>
        
        <service template="Template.WebServer.Log#ComErrorGroupCategory"/>
        <service template="Template.WebServer.Log#ComErrorFileCategory"/>
        <service template="Template.WebServer.Log#ComErrorConsoleCategory"/>
        
        <service template="Template.WebServer.Log#ComFatalGroupCategory"/>
        <service template="Template.WebServer.Log#ComFatalFileCategory"/>
        <service template="Template.WebServer.Log#ComFatalConsoleCategory"/>
        
        <service template="Template.WebServer.Log#ApplGroupCategory"/>
        <service template="Template.WebServer.Log#ApplFileCategory"/>
        <service template="Template.WebServer.Log#ApplConsoleCagegory"/>
        
    </manager>
    
</server>