<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE server PUBLIC
  "-//Nimbus//DTD Nimbus 1.0//JA"
  "http://nimbus.sourceforge.jp/dtd/nimbus-service_1_0.dtd">

<server>
    
    <default-log>
        <debug output="false"/>
        <information output="false"/>
        <warning output="true"/>
        <error output="true"/>
        <fatal output="true"/>
    </default-log>
    
    <manager name="Sample.Service.Queue">
        
        <service template="Template.Service.Queue.QueueHandlerContainer#QueueHandlerContainer">
            <attribute name="IgnoreNullElement">true</attribute>
            <attribute name="QueueHandlerSize">5</attribute>
        </service>
        
    </manager>
</server>